services:
  db:
    build:
      context: C:\Users\IS Aorus\source\repos\Gastrocafe\sqlserver
      dockerfile: Dockerfile
    volumes:
    - C:\Users\IS Aorus\source\repos\Gastrocafe\dockerDB:/var/opt/mssql/data:rw
  nginx-proxy:
    build:
      context: C:\Users\IS Aorus\source\repos\Gastrocafe\nginx
      dockerfile: Dockerfile
    links:
    - sanrafael
    ports:
    - published: 80
      target: 80
  sanrafael:
    build:
      context: C:\Users\IS Aorus\source\repos\Gastrocafe
      dockerfile: SanRafael/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: SanRafael
      target: base
    container_name: SanRafael
    depends_on:
      db:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DIRECTORIO: /gastrocafe
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    expose:
    - '80'
    image: sanrafael:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp2.0/SanRafael.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    tty: true
    volumes:
    - C:\Users\IS Aorus\source\repos\Gastrocafe\SanRafael:/app:rw
    - C:\Users\IS Aorus\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\IS Aorus\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\IS Aorus\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\IS Aorus\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\IS Aorus\source\repos\Gastrocafe:/src:rw
version: '3.4'
volumes:
  dockerBD: {}